# Environment Configuration for Yell
# Copy this to .env and fill in actual values
# NEVER commit .env to version control!

# Application
NODE_ENV=production

# Database
DB_USER=yell
DB_PASSWORD=CHANGE_ME_WITH_STRONG_PASSWORD
DB_NAME=yell
DB_PORT=5432

# Redis
REDIS_PASSWORD=CHANGE_ME_WITH_STRONG_PASSWORD
REDIS_PORT=6379

# URLs for Traefik
WEB_URL=yell.app
APP_URL=app.yell.app

# Traefik Let's Encrypt
ACME_EMAIL=admin@yell.app
